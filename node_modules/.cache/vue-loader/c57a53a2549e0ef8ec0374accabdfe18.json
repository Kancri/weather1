-->\n        <div class=\"comments\">\n         <h3>热门留言</h3> \n         <ul class=\"com_info\">\n           <li>\n            <img class=\"avatar\" src=\"../images/person.png\" alt=\"\">\n            <div class=\"person\"><b class=\"name\"> 镜子浮云</b>\n                                <p class=\"content\"> 这是一段评论内容，这是一段评论内容，\n         这是一段评论内容，，，</p>\n            </div>\n          </li>\n         </ul>\n        </div>\n      </section>\n     <!-- 下方播放控制 -->\n      <div class=\"audio_con\">\n        <audio controls = \"controls\" autoplay loop class=\"myaudio\"></audio>\n      </div>\n      <!-- mv播放界面 -->\n      <div class=\"video_con\" style=\"display:none;\" >\n        <video src=\"\" controls=\"controls\" class=\"myvideo\"></video>\n        <div class=\"mask\" ></div>\n      </div>\n    </div>\n  </div>\n</div>\n\n</template>\n\n<script>\n    export default {\n        name: \"App\",\n        data() {\n            return {\n                song: \"\",\n                musicList: [],\n            }\n        },\n        methods: {\n            searchMusic: function() {\n              var that = this;\n                this.axios.get(\"https://autumnfish.cn/search?keywords=\"+this.song).then(function(response) {\n                  // console.log(response);\n                  that.musicList = response.data.result.songs;\n                }, function(err) {})\n            }\n        },\n    };\n</script>\n\n<style>\n\n</style>"]}]}