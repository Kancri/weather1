    <button class=\"search\" cursor:pointer @click=\"searchWeather\">搜索</button>\n    </div>\n    <div class=\"hotcity\">\n      <a\n        href=\"javascript:;\"\n        v-for=\"city in hotCitys\"\n        :key=\"city\"\n        @click=\"changeCity(city)\"\n      >\n        {{ city }}\n      </a>\n    </div>\n\n    <ul class=\"weather_list\">\n      <li class=\"weather_info\" v-for=\"item in weatherList\" :key=\"item\">\n        <div class=\"weather\">\n          <span> {{ item.wea }} </span>\n        </div>\n        <div class=\"temp\">\n          <b>  {{ item.tem2 }} </b> ~\n          <b>  {{ item.tem1 }} </b>\n        </div>\n        <div class=\"date\">\n          <span>  {{ item.date + item.week }} </span>\n        </div>\n      </li>\n    </ul> \n  </div>\n</template>\n\n<script>\n/*\n  请求地址:https://tianqiapi.com/api?version=v6&appid=76228964&appsecret=FEemrZ5f\n  请求方法:get\n  请求参数:city(城市名)\n  响应内容:天气信息\n\n  1. 点击回车\n  2. 查询数据\n  3. 渲染数据\n  */\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      city: \"\",\n      weatherList: [],\n      hotCitys: [\"北京\", \"上海\", \"广州\", \"深圳\"],\n    };\n  },\n  methods: {\n    searchWeather:function(){\n      this.weatherList = [];\n      // console.log('天气查询');\n      // console.log(this.city);\n      var that = this;\n      this.axios.get('https://tianqiapi.com/api?version=v6&appid=76228964&appsecret=FEemrZ5f&city='+this.city)\n      .then(function(response){\n        that.weatherList = response.data\n      })\n      .catch(function(err){})\n    },\n    changeCity:function(city){\n      this.city = city;\n      this.searchWeather();\n    },\n    // cearchWeather: function () {\n    //   this.$http\n    //     .get( 'https://tianqiapi.com/api?version=v6&appid=76228964&appsecret=FEemrZ5f&city='+this.city )\n    //     .then((response) => {\n    //       console.log(\"成功\");\n    //     })\n    //     .catch((err) => {\n    //       console.log(\"失败\");\n    //     });\n    // },\n  },\n};\n</script>\n\n<style>\n\n</style>\n"],"sourceRoot":"src"}]}